/* eslint-disable */
/**
 * The javascript functions for the Standard Chartered Website
 * Contains body classes, anchor links, back to top, sliders,
 * lightboxes, accordions and dropdowns.
 *
 * @summary Javascript functions for SC.
 * @package standardchartered
 */

 /* global jQuery, nfRadio */

/*
 * Add body class on page load
 */
jQuery( window ).load( function () {
	jQuery( 'body' ).addClass( 'loaded' );
} );

jQuery( document ).ready( function ( $ ) {

	/*
	 * Language links dropdown
	 */
	$( '.language-trigger' ).click( function ( event ) {
		event.preventDefault();
		$( '.language-sub' ).slideToggle( 300 );
		$( '.page-active-overlay' ).toggleClass( 'overlay-active' );
	} );
	$( 'body' ).on( 'click', '.language-sub a', function ( event ) {
		$( '.language-sub' ).slideUp( 300 );
	} );
	/*$( document ).click( function ( event ) {
		if ( $( '.language-sub' ).is( ':visible' ) && $( event.target ).is( '.language-trigger' ) === false ) {
			$( '.language-sub' ).slideToggle( 300 );
		}
	} );*/

	/*
	 * Language auto redirect
	 */
	function redirectToAnotherLanguage() {
		var langcookiename = 'langCountry_' + window.location.pathname.replace( /\//g, '_' );
		if ( getCookie( langcookiename ) ) {
			return false;
		}

		$( 'body' ).css( 'opacity', '0' );
		geoip2.country( function ( location ) {
			var langCountry = location.country.iso_code;
			setCookie( langcookiename, langCountry, 7 );
			if ( $( '.language-sub a[data-country-code*=",' + langCountry + '"]' ).length ) {
				var langcookienameHome = 'langCountry_' + $( '.language-sub a[data-country-code*=",' + langCountry + '"]:first' ).attr( 'href' ).replace( /^.*\/\/[^\/]+/, '' ).replace( /\//g, '_' );
				if ( getCookie( langcookienameHome ) === '' ) {
					window.location = $( '.language-sub a[data-country-code*=",' + langCountry + '"]:first' ).attr( 'href' );
				} else {
					$( 'body' ).css( 'opacity', '1' );
				}
			} else {
				$( 'body' ).css( 'opacity', '1' );
			}
		}, function ( error ) {
			$( 'body' ).css( 'opacity', '1' );
		} );
	}
	if ( $( '.language-sub a' ).length && typeof geoip2 !== 'undefined' ) {
		redirectToAnotherLanguage();
	}

	function navigationListLinks() {

		/*
		 * Show anchor navigation
		 */
		if ( $( '#mega-secondary-menu' ).length ) {
			$( '.anchor-navigation' ).css( 'display', 'block' );
		} else {
			$( '.anchor-navigation' ).css( 'display', 'none' );
		}

		/*
		 * Get all data attributes for dropdown menu
		 */
		var anchorlink = $( '.sc-anchor-link:visible' ).map( function () {
			var scdataid = $( this ).attr( 'data-id' ),
				scid = $( this ).attr( 'id' );
			return '<li><a href="#' + scid + '">' + scdataid + '</a></li>';
		} );

		$( '.anchor-navigation .anchor-sub' ).html( anchorlink.get().join( ' ' ) );

	}

	navigationListLinks();
	$( '.sc-tabs-list .sc-tab' ).click( function () {
		setTimeout( function () {
			navigationListLinks();
		}, 100 );
	} );

	/*
	 * Search Animation
	 */
	$( '.select-country a,#primary-menu .countryselect a, .eyebrow-local a, .mobile-local a' ).click( function ( e ) {
		e.preventDefault();
		$( '.find-country' ).focus();
		$( '#header-country-select' ).toggleClass( 'active' );
		$( '.eyebrow-local a' ).toggleClass( 'active-eyebrow' );
		$( '.online-location-dropdown' ).removeClass( 'active-online-dropdown' );
		$( '.eyebrow-online a' ).removeClass( 'active-eyebrow' );
		$( '#sc-mega-menu .menu-item-has-children' ).removeClass( 'activated-sub' );
		$( '#sc-mega-menu .menu-item-has-children .mega-nav-intro' ).removeClass( 'mega-nav-active' );
		$( '.mega-sub-panel' ).removeClass( 'mega-panel-active' );

		if ( $( '.eyebrow-local a' ).hasClass( 'active-eyebrow' ) ) {
			$( '.page-active-overlay' ).addClass( 'overlay-active' );
		} else {
			$( '.page-active-overlay' ).removeClass( 'overlay-active' );
		}
	} );

	$( '.mobile-local a' ).click( function ( e ) {
		$( '#sc-mega-menu .sub-main-overlay' ).addClass( 'overlay-toggled-open' );
	} );

	$( '.eyebrow-online a, .mobile-online a' ).click( function ( e ) {
		$( '#sc-mega-menu .sub-main-overlay' ).addClass( 'overlay-toggled-open' );
		$( '#sc-mega-menu .menu-item-has-children .mega-nav-intro' ).removeClass( 'mega-nav-active' );
		$( '.mega-sub-panel' ).removeClass( 'mega-panel-active' );
	} );

	$( '.toggle-close' ).click( function () {
		$( '.find-country' ).blur();
		$( '#header-country-select' ).toggleClass( 'active' );
		$( '.page-active-overlay' ).toggleClass( 'overlay-active' );
		$( '.eyebrow-local a' ).toggleClass( 'active-eyebrow' );
	} );

	$( '.page-active-overlay' ).click( function () {
		$( this ).toggleClass( 'overlay-active' );
		$( '.find-country' ).blur();
		$( '#header-country-select' ).removeClass( 'active' );
		$( '.mega-sub-panel' ).removeClass( 'mega-panel-active' );
		$( '.online-location-dropdown' ).removeClass( 'active-online-dropdown' );
		$( '.eyebrow-navigation a' ).removeClass( 'active-eyebrow' );
		$( '.language-sub' ).slideUp( 300 );
	} );

	$( '.eyebrow-online a, .mobile-online a' ).click( function ( e ) {
		e.preventDefault();
		$( '.online-location-dropdown' ).toggleClass( 'active-online-dropdown' );
		$( '.eyebrow-online a' ).toggleClass( 'active-eyebrow' );
		$( '.eyebrow-local a' ).removeClass( 'active-eyebrow' );
		$( '#header-country-select' ).removeClass( 'active' );
		$( '#sc-mega-menu .menu-item-has-children' ).removeClass( 'activated-sub' );
		$( '#sc-mega-menu .sub-main-overlay' ).addClass( 'overlay-toggled-open' );


		if ( $( '.eyebrow-online a' ).hasClass( 'active-eyebrow' ) ) {
			$( '.page-active-overlay' ).addClass( 'overlay-active' );
		} else {
			$( '.page-active-overlay' ).removeClass( 'overlay-active' );
		}
	} );

	$( '#sc-mega-menu' ).append( '<div class="sub-main-overlay"></div>' );
	$( '.sub-menu' ).append( '<div class="sub-overlay"></div>' );
	$( '.online-location-dropdown' ).append( '<div class="online-location-back"><span></span>Back</div>' );
	$( '#header-country-select' ).append( '<div class="header-country-back">Back</div>' );

	$( '.online-location-back' ).click( function () {
		$( this ).parent().removeClass( 'active-online-dropdown' );
		$( '#sc-mega-menu .sub-main-overlay' ).removeClass( 'overlay-toggled-open' );
	} );

	$( '.header-country-back' ).click( function () {
		$( '.find-country' ).blur();
		$( '#header-country-select' ).toggleClass( 'active' );
		$( '#sc-mega-menu .sub-main-overlay' ).removeClass( 'overlay-toggled-open' );
	} );

	$( '.menu-toggle, .page-active-overlay' ).click( function () {

		if ( $( '#header-country-select' ).hasClass( 'active' ) ) {
			$( '.find-country' ).blur();
			$( '#header-country-select' ).removeClass( 'active' );
		}

		if ( $( '.online-location-dropdown' ).hasClass( 'active-online-dropdown' ) ) {
			$( '.online-location-dropdown' ).removeClass( 'active-online-dropdown' );
		}

		if ( $( '#sc-mega-menu .sub-main-overlay' ).hasClass( 'overlay-toggled-open' ) ) {
			$( '#sc-mega-menu .sub-main-overlay' ).removeClass( 'overlay-toggled-open' );
		}

		if ( $( '#sc-mega-menu .menu-item-has-children' ).hasClass( 'activated-sub' ) ) {
			$( '#sc-mega-menu .menu-item-has-children' ).removeClass( 'activated-sub' );
		}

		if ( $( '#sc-mega-menu .menu-item-has-children' ).hasClass( 'focus' ) ) {
			$( '#sc-mega-menu .menu-item-has-children' ).removeClass( 'focus' );
		}

		if ( $( '#sc-mega-menu .sub-overlay' ).hasClass( 'overlay-toggled-open' ) ) {
			$( '#sc-mega-menu .sub-overlay' ).removeClass( 'overlay-toggled-open' );
		}

		if ( $( '.mega-nav-intro' ).hasClass( 'mega-nav-active' ) ) {
			$( '.mega-nav-intro' ).removeClass( 'mega-nav-active mega-nav-reduced' );
		}

		if ( $( '.page-active-overlay' ).hasClass( 'overlay-active' ) ) {
			$( '.page-active-overlay' ).removeClass( 'overlay-active' );
		}

	} );

	$( 'body' ).click( function( evt ) {
		if ( !$( evt.target ).is( '#sc-mega-menu' ) ) {
			if ( $( '.mega-nav-intro' ).hasClass( 'mega-nav-active' ) ) {
				$( '.mega-nav-intro' ).removeClass( 'mega-nav-active mega-nav-reduced' );
			}
		}
	} );

	$( '#sc-mega-menu > .menu-item-has-children > a' ).on( 'click', function ( e ) {
		e.preventDefault();

		$( this ).parent().parent().find( '.sub-main-overlay' ).addClass( 'overlay-toggled-open' );
		$( '#sc-mega-menu' ).animate( { scrollTop: 0 } );
		if ( $( this ).parent().hasClass( 'activated-sub' ) ) {
			$( this ).parent().removeClass( 'activated-sub' );
			$( '.page-active-overlay' ).removeClass( 'overlay-active' );
			$( this ).parent().find( '.mega-nav-intro' ).removeClass( 'mega-nav-active mega-nav-reduced mega-nav-hidden' );
		} else {
			$( '.page-active-overlay' ).addClass( 'overlay-active' );
			$( '.menu-item-has-children' ).removeClass( 'activated-sub' );
			$( '.mega-nav-intro' ).removeClass( 'mega-nav-active mega-nav-reduced mega-nav-hidden' );
			$( this ).parent().children( '.mega-nav-intro' ).addClass( 'mega-nav-active' );
			$( this ).parent().toggleClass( 'activated-sub' );
		}

		if ( $( '.nav-heading' ).hasClass( 'activated-sub' ) ) {
			$( '.mega-sub-panel' ).addClass( 'mega-panel-active' );
		} else {
			$( '.mega-sub-panel' ).removeClass( 'mega-panel-active' );
		}
	} );

	$( '.sub-menu .menu-item-has-children > a' ).on( 'click', function ( e ) {

		$( '.mega-nav-intro' ).first().addClass( 'mega-nav-active' );
		if (  $( this ).closest( 'li' ).hasClass( 'activated-sub' ) ) {
			// Set item inactive.
			setMeganavInactive( $( this ).closest( 'li' ) );

		} else {
			// Set sibling items inactive.
			$( this ).closest( 'li' ).siblings().each( function( index, liElement ) {
				setMeganavInactive( liElement );
			} );

			// Set item active.
			setMeganavActive( $( this ).closest( 'li' ) );
		}
		$( '#sc-mega-menu .nav-heading' ).find( '> .mega-nav-intro' ).removeClass( 'mega-nav-active' );
	} );

	// Remove active classes all the way down inside liElement.
	function setMeganavInactive ( liElement ) {
		// Add inactive classes.
		$( liElement ).removeClass( 'activated-sub' );
		$( '.mega-nav-intro', liElement ).removeClass( 'mega-nav-active' );
	}

	$( '.tier-two' ).on( 'click touch', function() {
		$( this ).parent().parent().find( '.mega-nav-intro' ).toggleClass( 'mega-nav-active' );
	} );

	// Add active classes immediately down inside liElement.
	function setMeganavActive ( liElement ) {
		// Add active classes.
		$( '.tier-two' ).removeClass( 'activated-sub unactivated-sub' );
		$( liElement ).addClass( 'activated-sub' );
		$( '> .sub-menu', liElement ).addClass( 'mega-nav-active' );
		$( '> .mega-nav-intro', liElement ).addClass( 'mega-nav-reduced' );
		$( '> .mega-nav-intro', liElement ).addClass( 'mega-nav-active' );
	}

	$( '.tier-one a:not(.back-trigger)' ).on( 'click touch', function() {

		if( ! $( this ).parent().find( '> .sub-menu' ).hasClass( 'sub-active' ) ) {

			$( '.tier-two' ).removeClass( 'activated-sub unactivated-sub' );

			$( this ).parent().find( '.mega-nav-intro' ).addClass( 'mega-nav-active' );
			setMeganavActive( $( this ).closest( 'li' ) );
			$( '.depth-2' ).removeClass( 'mega-nav-reduced mega-nav-active' );
		}

		$( '.mega-nav-intro' ).removeClass( 'mega-nav-active' );
	} );

	$( '.tier-one.menu-item-has-children > a:not(.back-trigger)' ).on( 'hover mouseover', function() {
		if( ! $( '.sub-active .sub-active' ).length ) {
			$( this ).parent().children( '.mega-nav-intro' ).addClass( 'mega-nav-active' );
			$( this ).parent().parent().parent().children( '.mega-nav-intro' ).removeClass( 'mega-nav-active' );
		}
	} );

	$( '<li class="mega-sub-back"><a class="back-trigger" href="#">Back</a></li>' ).insertBefore( '#sc-mega-menu .sub-overlay' );

	$( '#sc-mega-menu .sub-menu > .mega-sub-back > a' ).click( function ( e ) {
		e.preventDefault();
		$( this ).parent().parent().removeClass( 'mega-nav-active' );
		$( this ).parent().parent().parent().removeClass( 'activated-sub' );
		$( this ).parent().parent().parent().removeClass( 'focus' );
		$( this ).parent().parent().parent().parent().find( '.sub-main-overlay' ).removeClass( 'overlay-toggled-open' );
		$( this ).parent().parent().parent().parent().find( '.sub-overlay' ).removeClass( 'overlay-toggled-open' );
	} );

	$( '#sc-mega-menu .sub-menu .sub-menu > .mega-sub-back > a' ).click( function ( e ) {
		e.preventDefault();
		$( this ).parent().parent().removeClass( 'mega-nav-active' );
		$( this ).parent().parent().parent().removeClass( 'activated-sub' );
		$( this ).parent().parent().removeClass( 'focus' );
		$( this ).parent().parent().parent().parent().find( '.sub-main-overlay' ).removeClass( 'overlay-toggled-open' );
		$( this ).parent().parent().parent().parent().find( '.sub-overlay' ).removeClass( 'overlay-toggled-open' );
	} );

	$( '#content' ).on( 'click touch', function() {
		$( '.mega-nav-intro' ).removeClass( 'mega-nav-active' );
	} );

	if ( $( '.sub-header' ).length ) {
		$( '#content' ).addClass( 'main-with-sub-header' );
	}

	if ( window.matchMedia( '(max-width: 799px)' ).matches ) {
		$( '.sub-menu' ).swipe( {
			swipe:function( e, direction ) {
				if ( direction === 'left' ) {
					e.stopPropagation();
					e.stopImmediatePropagation();
					$( this ).removeClass( 'mega-nav-active' );
					$( this ).parent().parent().addClass( 'mega-nav-active' );
					$( this ).parent().removeClass( 'activated-sub' );
					$( this ).parent().parent().find( '.sub-main-overlay' ).removeClass( 'overlay-toggled-open' );
					$( this ).parent().parent().find( '.sub-overlay' ).removeClass( 'overlay-toggled-open' );
				}
			}
		} );
	}

	if ( ! $( '.fl-builder-bar' ).length && ! $( '.sc-preview' ).length ) {
		var sticky = new Waypoint.Sticky( {
			element: $( '#masthead' )[0], // Add in your CSS selector sticky target
			direction: 'down right',
		} );
	}

	/*
	 * Mega nav min heights setter
	 */
	$( window ).on( 'load resize', function () {
		megaNavMinHeight();
	} );

	function megaNavMinHeight() {
		if ( $( '.eyebrow-present .mobile-navigation .hamburger' ).is( ':visible' ) ) {
			// Clear inline any min-heights for mobile.
			$( '.eyebrow-present .main-navigation ul .menu-item-has-children .sub-menu' ).css( 'min-height', '' );
			$( '.mega-nav-intro' ).css( 'height', '' );
			$( '.mega-sub-panel' ).css( 'height', '' );
		} else {
			var minHeight = 320;

			// Find the largest required min-height.
			$( '.eyebrow-present .main-navigation ul .menu-item-has-children .sub-menu' ).each( function() {
				// Reset styles before check.
				$( this ).css( 'min-height', '' ).css( 'top', 'auto' ).css( 'bottom', 'auto' );

				// Check if this this is biggest so far.
				minHeight = minHeight > $( this ).height() ? minHeight : $( this ).height();

				// Restore top and bottom to stylesheet values.
				$( this ).css( 'top', '' ).css( 'bottom', '' );
			} );

			// Apply new min-height.
			$( '.eyebrow-present .main-navigation ul .menu-item-has-children .sub-menu' ).css( 'min-height', minHeight );
			$( '.mega-nav-intro' ).css( 'height', minHeight );
			$( '.mega-sub-panel' ).css( 'height', minHeight );
		}
	}

	/*
	 * Smooth scroll to anchor links
	 */
	$( 'body' ).on( 'click', 'a[href*=\\#]:not([href=\\#] )', function () {
		if ( $( this ).closest( '.fl-lightbox-content' ).length ) {
			return true;
		}
		if ( location.pathname.replace( /^\//, '' ) === this.pathname.replace( /^\//, '' ) && location.hostname === this.hostname ) {
			var target = $( this.hash );
			target = target.length ? target : $( '[name=' + this.hash.slice( 1 ) + ']' );
			if ( target.length && ! ( $( this ).hasClass( 'screen-reader-text' ) ) ) {
				$( 'html,body' ).animate( { scrollTop: target.offset().top }, 1000 );
				return false;
			}
		}
	} );

	/*
	 * Anchor links dropdown
	 */
	$( '.anchor-trigger' ).click( function ( event ) {
		event.preventDefault();
		$( '.anchor-sub, .anchor-sub-nav' ).slideToggle( 300 );
		$( '.anchor-navigation' ).toggleClass( 'anchor-open' );
		$( '.page-active-overlay' ).toggleClass( 'overlay-active' );
	} );
	$( 'body' ).on( 'click', '.anchor-sub a', function ( event ) {
		event.preventDefault();
		$( '.anchor-sub, .anchor-sub-nav' ).slideUp( 300 );
		$( '.anchor-navigation' ).removeClass( 'anchor-open' );
		$( '.page-active-overlay' ).removeClass( 'overlay-active' );
	} );
	$( document ).click( function ( event ) {
		if ( $( '.anchor-sub, .anchor-sub-nav' ).is( ':visible' ) && $( event.target ).is( '.anchor-trigger' ) === false ) {
			$( '.anchor-sub, .anchor-sub-nav' ).slideUp( 300 );
			$( '.anchor-navigation' ).removeClass( 'anchor-open' );
			$( '.page-active-overlay' ).removeClass( 'overlay-active' );
		}
	} );

	/*
	 * Back to top
	 */
	var ScrollTopDuration = 700,
	    $BackToTop = $( '.sc-top' );

	$BackToTop.on( 'click', function ( event ) {
		event.preventDefault();
		$( 'body,html' ).animate( { scrollTop: 0 }, ScrollTopDuration );
	} );

	/*
	 * Back to top hide and show based upon scrolling
	 * Disabled temporarily for rebrand.
	 * /
	var stickyTop = $( '.content-area' ).offset().top;
	$( window ).scroll( function () {
		if ( $( window ).scrollTop() >= stickyTop ) {
			$( '.back-to-top' ).fadeIn( 200 );
		} else {
			$( '.back-to-top' ).fadeOut( 200 );
		}
	} );*/

	/*
	 * iFrame fix
	 */
	$( '.sc-content-blocks-iframe iframe[src*="eurolandir"]' ).EurolandIFrameAutoHeight();
	$( 'iframe' ).removeAttr( 'frameborder' );

	/*
	 * Animated Numbers
	 */
	$( '.stats-slider' ).waypoint( function () {
		$( '.timers' ).countTo( {
			speed:     1000,
			formatter: function ( value, options ) {
				let getToNumber = $( this ).attr( 'data-to' );

				if ( getToNumber % 1 !== 0 ) {
					var numberOfDecimals = getToNumber.toString().split(".")[1].length;
					options.decimals = numberOfDecimals;
				}
				value = value.toFixed( options.decimals );
				if ( ! numberOfDecimals ) {
					value = value.replace( /\B(?=(\d{3})+(?!\d))/g, ',' );
				}
				return value;
			},
		} );
	}, { offset: '100%' } );

	/*
	 * YouTube Popup using Magnific Popup
	 */
	$( '.popup-youtube' ).magnificPopup( {
		type:            'iframe',
		mainClass:       'mfp-fade',
		removalDelay:    160,
		preloader:       false,
		fixedContentPos: false,
		iframe:          {
			patterns: {
				youtube: {
					index: 'youtube.com/',
					id:    'v=',
					src:   '//www.youtube.com/embed/%id%?rel=0&showinfo=0&autoplay=1',
				},
			},
			srcAction: 'iframe_src',
		},
	} );

	/*
	 * Youtube lightbox auto-popup
	 */
	$( '.popup-youtube' ).click( function() {
		var get_video_url = $( this ).attr( 'href' );
		var video_hash = get_video_url.substring( 31, ( get_video_url.length - get_video_url.indexOf( '#' ) ) );
		window.location.hash = 'youtube-' + video_hash;
	});
	if ( !String.prototype.startsWith ) {
	  String.prototype.startsWith = function( searchString, position ) {
	    position = position || 0;
	    return this.indexOf( searchString, position ) === position;
	  };
	}
	if ( window.location.hash.startsWith( '#youtube-' ) ) {
		var video_hash = window.location.hash.substring( 9, ( window.location.hash.length - window.location.hash.indexOf( '#' ) ) );
		var youtube_link = $( 'a.popup-youtube[ href*="' + video_hash + '" ]' );
		youtube_link.trigger( 'click' );
		if ( youtube_link.closest( '.tab-wrap > .panel' ) ) {
			// Video is inside a tab, open tab
			window.showtab( '#' + youtube_link.closest( '.tab-wrap > .panel' ).attr( 'id' ) );
		}
	}

	/*
	 * Country selector & asset Popup
	 */
	$( 'a[href*="#country-popup-"]' ).each( function ( index, value ) {
		$( value ).addClass( 'country-popup' ).attr( 'data-location', $( value ).attr( 'href' ).replace( '#country-popup-', '' ) ).attr( 'href', '#country-popup' );
	} );
	$( '.country-popup' ).click( function () {
		$( '#country-popup .countrypopup-service' ).hide();
		var service = ( $( this ).data( 'location' ) === 'open-country-popup' ) ? 'business' : $( this ).data( 'location' );
		$( '#country-popup #countrypopup-service-' + service ).show();
	} );
	$( '.country-popup, .asset-popup, .content-lightbox' ).magnificPopup( {
		type:      'inline',
		preloader: false,
	} );

	/*
	 * Specific lightbox with preloader
	 */
	$( '.sc-video-with-lightbox .content-lightbox' ).magnificPopup( {
		type: 'inline',
		preloader: true
	} );

	/*
	 * Asset button disable
	 */
	$( '.enable' ).click( function () {
		if ( ! $( this ).is( ':checked' ) ) {
			$( '.btn-target' ).removeClass( 'btn-target-enabled' ).addClass( 'btn-target-disabled' );
		} else if ( $( this ).not( ':checked' ) ) {
			$( '.btn-target' ).removeClass( 'btn-target-disabled' ).addClass( 'btn-target-enabled' );
		}
	} );

	/*
	 * Local storage for downloads
	 */
	var DStatus = localStorage.getItem( 'scdocactive' );
	if ( DStatus === 'true' ) {
		$( '.enable' ).prop( 'checked', true );
		$( '.btn-target' ).attr( 'disabled', false );
	}
	$( '.enable' ).click( function () {
		localStorage.setItem( 'scdocactive', $( this ).prop( 'checked' ) );
	} );

	/*
	 * SC Accessible Accordions
	 */
	/*global show sctheme */

	$( '.accordion-title a' ).on( 'click', function ( event ) {
		event.preventDefault();
	} );
	$( '.accordion-title' ).on( 'focus', function () {
		if ( ! $( this ).data( 'mouseDown' ) && ! $( this ).hasClass( 'open' ) ) {
			$( this ).click();
		}
	} );

	$( '.accordion-title' ).on( 'mousedown', function () {
		$( this ).data( 'mouseDown', true );
	} );

	$( '.accordion-title' ).on( 'mouseup', function () {
