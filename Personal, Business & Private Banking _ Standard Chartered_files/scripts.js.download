/**
 * Option filter used on financial results page, it can be used on another page by using custom filter.
 * This code will only run if the page contains either 'financial-results-heading-' or 'custom-filter-heading-' IDs
 */
if( jQuery( '[id^="financial-results-heading-"], [id^="custom-filter-heading-"]' ) ) {
	var sectionHeader = jQuery( '[id^="financial-results-heading-"], [id^="custom-filter-heading-"]' );
	var partOfTheIDName = jQuery( sectionHeader ).get(0).id.trim().replace( /-heading.*/,'' );

	// Create select options and append option values.
	jQuery( '[id^="' + partOfTheIDName + '-heading-"]' ).first().parent().prepend( '<select class="thefilters"><option value="">show all</option></select>' );

	if( partOfTheIDName == 'financial-results' ) {
		sectionHeader.each(function(index) {
			var sectionHeaderValue = jQuery( sectionHeader[index] ).text().trim().replace( /\D/g,'' );
			jQuery( '.thefilters' ).append( '<option value="' + sectionHeaderValue + '">' + sectionHeaderValue + '</option>' );
		});
	} else {
		sectionHeader.each(function(index) {
			var sectionHeaderName = jQuery( sectionHeader[index] ).text().trim();
			var sectionHeaderValue = jQuery( sectionHeader[index] ).get(0).id.trim().replace( 'custom-filter-heading-', '' );

			jQuery( '.thefilters' ).append( '<option value="' + sectionHeaderValue + '">' + sectionHeaderName + '</option>' );
		});
	}

	// Change visibility based on the option selected.
	jQuery( '.thefilters' ).change(function() {
		var selectedOptions = jQuery( this ).val();
		if ( selectedOptions == "" ) {
			jQuery( '[class*="'+ partOfTheIDName + '-content-"]' ).show();
		} else {
			jQuery( '[class*="'+ partOfTheIDName + '-content-"]' ).hide();
			jQuery( '.'+ partOfTheIDName + '-content-' + selectedOptions ).show();
		}
	});
}

/**
